PROGRAM step4
BEGIN

	INT a, b, i, p;
	STRING eol := "\n";
	STRING space := " ";

	FUNCTION VOID main()
	BEGIN

		i := 0;
		a := 0;
		b := 0;

		DO
			READ (p);
			
			IF (p > 10) 
				a := a + 1;
			ELSIF (TRUE)
      			b := b + 1;
			ENDIF

			i := i+1;
			WRITE(i,space,a,space,b,eol);

		WHILE (i != 10);

		WRITE (a);
		WRITE (b);
	END
END
