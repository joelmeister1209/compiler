PROGRAM test
BEGIN
	STRING space := " ";
	STRING eol := "\n";
	INT i, j;
	FLOAT newapprox,approx,num;

	FUNCTION VOID main()
	BEGIN
	
		num := 7.0;	
		i := 100;
		j := 1;
		approx := num;	
	
          DO
			newapprox := 0.5*(approx + num/approx);
			approx := newapprox;
			i := i-1;
			WRITE(i,space,newapprox,space,approx,eol);
		WHILE (i != 0);

		WRITE(approx);
	END
END
